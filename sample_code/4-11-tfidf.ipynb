    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "\n",
    "vec = TfidfVectorizer(input='filename')\n",
    "bow = vec.fit_transform(['sample_text.txt']) \n",
    "\n",
    "\n",
    "# 抽出した単語\n",
    "words = vec.get_feature_names()\n",
    "counts = bow.toarray()[0]\n",
    "\n",
    "# 重み付けられた単語\n",
    "for i in range(10):\n",
    "    print(words[i], counts[i])\n"
