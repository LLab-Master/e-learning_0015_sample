   "source": [
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "\n",
    "vec = CountVectorizer(input='filename')\n",
    "bow = vec.fit_transform(['sample_text.txt'])\n",
    "\n",
    "# 抽出した単語\n",
    "words = vec.get_feature_names()\n",
    "counts = bow.toarray()[0]\n",
    "\n",
    "# 単語出現回数(抜粋)\n",
    "for i in range(10):\n",
    "    print(words[i], counts[i])\n"
   ]
